<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ivysettings>
	<settings defaultResolver="ibiblio"/>
	<resolvers>
		<ibiblio name="ibiblio" m2compatible="true"/>
        <ibiblio name="swt" m2compatible="true" root="https://swt-repo.googlecode.com/svn/repo/"/>
        <url name="nrjavaserial">
            <artifact pattern="http://nrjavaserial.googlecode.com/files/[artifact]-[revision].[ext]" />
        </url>
    </resolvers>
	<modules>
        <module organisation="org.eclipse.swt" name="*" resolver="swt" />
        <module organisation="com.neuronrobotics" name="nrjavaserial" resolver="nrjavaserial" />
	</modules>
</ivysettings>